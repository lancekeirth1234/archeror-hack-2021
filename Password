var password = "Archeror";
var response;
var entryCount = 6;
var entryLimit = 1000000;
var error = false;

while(response != password && !error){
if(entryCount < entryLimit){
        response = window.prompt("Enter Password");
        entryCount++;
    } else {
        error = true;
    }
}

if(error){
    alert("Wrong Pass Try Again");
} else {
    alert("Good job!");
}
